<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.namebattler_ver00">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.CreatedCharaActivity" />
        <activity android:name=".Activities.BattleResultActivity" />
        <activity android:name=".Activities.BattleActivity" />
        <activity android:name=".Activities.SelectTargetPartyActivity" />
        <activity android:name=".Activities.CreatePartyActivity" />
        <activity android:name=".Activities.CreateCharaActivity" />
        <activity android:name=".Activities.ShowCharaActivity" />
        <activity android:name=".Activities.ShowCharaListActivity" />
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>